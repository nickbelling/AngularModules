{
  "name": "@my-project/build",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc --build tsconfig.build.json && node copy-package.js"
  },
  "type": "commonjs",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@angular-architects/native-federation": "^17.1.3"
  },
  "sideEffects": false,
  "module": "index.js",
  "typings": "index.d.ts",
  "main": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.js",
      "default": "./src/index.ts"
    }
  }
}
